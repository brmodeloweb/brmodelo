{
	"presets": [
		//"@babel/env",
		"@babel/react",
    [
      "@babel/preset-env",
      {
        "modules": false
      },
      "jest"
    ]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "angularjs-annotate"
	],
	"env": {
    "test": {
			"presets": [
        [
          "@babel/preset-env",
          {
            "modules": "auto"
          },
          "jest"
        ]
      ],
      "plugins": ["@babel/plugin-transform-runtime"]
    }
  }
}